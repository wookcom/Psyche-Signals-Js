    "ux",
    "behavior",
    "intent",
    "analytics",
    "affective-computing",
    "cursor-tracking",
    "signals"
  ],
  "author": "Walter Sandoval",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^4.9.0",
    "@rollup/plugin-node-resolve": "^15.2.3"
  }
}