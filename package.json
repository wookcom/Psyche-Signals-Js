{
  "name": "psyche-signals",
  "version": "3.0.0",
  "description": "Real-time behavioral signals for adaptive UX",
  "type": "module",
  "main": "dist/psyche.cjs.js",
  "module": "dist/psyche.esm.js",
  "unpkg": "dist/psyche.umd.js",
  "exports": {
    ".": {
      "import": "./dist/psyche.esm.js",
